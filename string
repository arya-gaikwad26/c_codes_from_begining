#include<stdio.h>    
#include <string.h>      
int main(){      //main function  
  char ch[11]={ 't', 'p', 'o', 'i', 'n', 't', 't', 'e', 'c', 'h', '\0'};      
   char ch2[11]="tpointtech";      
      
   printf("Char Array Value is: %s\n", ch);      
   printf("String Literal Value is: %s\n", ch2);      

   
 return 0;      
}      
